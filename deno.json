{
	"tasks": {
		"start": "deno run -A --watch=static/,routes/ dev.ts",
		"test": "deno run -A enzyme"
	},
	"importMap": "./import_map.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	},
	"fmt": {
		"options": {
			"useTabs": true,
			"lineWidth": 80,
			"indentWidth": 4,
			"singleQuote": false,
			"proseWrap": "preserve"
		}
	},
	"setupFiles": []
}
